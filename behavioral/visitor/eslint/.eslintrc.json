{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "plugins": ["custom-rule"],
    "rules": {
        "custom-rule/name-rule": 1
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    }

}